<script>
function fmt(n) {
  return '$' + Math.round(n).toLocaleString();
}
function fmtPct(n) { return n + '%'; }

function update() {
  const hours   = parseFloat(document.getElementById('hours').value);
  const callLen = parseFloat(document.getElementById('callLen').value);
  const util    = parseFloat(document.getElementById('util').value) / 100;
  const show    = parseFloat(document.getElementById('show').value) / 100;
  const close   = parseFloat(document.getElementById('close').value) / 100;
  const price   = parseFloat(document.getElementById('price').value) || 0;
  const cashPct = parseFloat(document.getElementById('cashPct').value) / 100;
  const comm    = parseFloat(document.getElementById('comm').value) / 100;

  // Update display labels
  document.getElementById('v-hours').textContent = hours;
  document.getElementById('v-call-len').textContent = callLen.toFixed(2);
  document.getElementById('v-util').textContent = Math.round(util * 100) + '%';
  document.getElementById('v-show').textContent = Math.round(show * 100) + '%';
  document.getElementById('v-close').textContent = Math.round(close * 100) + '%';
  document.getElementById('v-comm').textContent = Math.round(comm * 100) + '%';

  // Core calculations
  const availCalls = hours / callLen;
  const bookedCalls = availCalls * util;
  const showedCalls = bookedCalls * show;
  const closedCalls = showedCalls * close;

  const cashPerDeal = price * cashPct;
  const commPerClose = cashPerDeal * comm;
  const weeklyEarn = closedCalls * commPerClose;
  const monthlyEarn = weeklyEarn * 4.33;
  const annualEarn = weeklyEarn * 52;

  document.getElementById('v-per-close').textContent = fmt(commPerClose);

  // Funnel
  document.getElementById('f-booked').textContent = bookedCalls.toFixed(1);
  document.getElementById('f-showed').textContent = showedCalls.toFixed(1);
  document.getElementById('f-closed').textContent = closedCalls.toFixed(1);

  const maxVal = Math.max(bookedCalls, 1);
  const barMax = 72;
  document.getElementById('fb-booked').style.height = (bookedCalls / maxVal * barMax) + 'px';
  document.getElementById('fb-showed').style.height = (showedCalls / maxVal * barMax) + 'px';
  document.getElementById('fb-closed').style.height = (closedCalls / maxVal * barMax) + 'px';

  document.getElementById('funnel-note').textContent =
    `Of ${bookedCalls.toFixed(1)} calls booked, ${showedCalls.toFixed(1)} show up and ${closedCalls.toFixed(1)} close ‚Äî per week`;

  // Results
  document.getElementById('r-weekly-sales').textContent = closedCalls.toFixed(1);
  document.getElementById('r-weekly-earn').textContent = fmt(weeklyEarn);
  document.getElementById('r-monthly-earn').textContent = fmt(monthlyEarn);
  document.getElementById('r-annual-earn').textContent = fmt(annualEarn);
}

update();
</script>

